{
  "org": "$HZN_ORG_ID",
  "label": "$SERVICE_NAME for $ARCH",
  "url": "$SERVICE_NAME",
  "version": "$SERVICE_VERSION",
  "arch": "$ARCH",
  "public": true,
  "sharable": "singleton",
  "requiredServices": [],
  "userInput": [],
  "deployment": {
    "services": {
      "$SERVICE_NAME": {
        "image": "$SERVICE_CONTAINER",
	"binds": ["$MMS_HELPER_SHARED_VOLUME:/mms-shared:rw", "/root/certs:/server/certs"],
        "ports": [ 
          { "HostIP": "0.0.0.0", "HostPort": "80:80/tcp" },
          { "HostIP": "0.0.0.0", "HostPort": "443:443/tcp" }
        ],
        "privileged": true
      }
    }
  }
}

